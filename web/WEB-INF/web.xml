        <servlet-class>controller.RegistrarMaestro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadExcel</servlet-name>
        <servlet-class>controller.UploadFileCostos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UplaodExcel2</servlet-name>
        <servlet-class>controller.UploadFlleJira</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarFechas</servlet-name>
        <servlet-class>controller.RegistrarFechas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadExcelNovedad</servlet-name>
        <servlet-class>controller.UploadExcelNovedad</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadExcelNombres</servlet-name>
        <servlet-class>controller.UploadExcelNombres</servlet-class>
