        <servlet-name>GuardarFechasArchivos</servlet-name>
        <url-pattern>/GenerarFechasArchivos.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SeleccionarFechas</servlet-name>
        <url-pattern>/SeleccionarFechas.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
